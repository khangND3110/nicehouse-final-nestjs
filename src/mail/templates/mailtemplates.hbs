<table width="100%" border="0" align="center" cellpadding="0" cellspacing="0">
    <tbody>
        <tr>
            <td align="center">
                <table class="col-600" width="600" border="0" align="center" cellpadding="0" cellspacing="0">
                    <tbody>
                        <tr>
                            <td align="center" valign="top"
                                style="background-size:cover; background-color: #f7f3e9; background-position:top; height: 400px">
                                <table class="col-600" width="600" height="400" border="0" align="center"
                                    cellpadding="0" cellspacing="0"
                                    style="border-left: 1px solid #dbd9d9; border-right: 1px solid #dbd9d9; border-top: 1px solid #dbd9d9;">
                                    <tbody>
                                        <tr>
                                            <td align="center" style="line-height: 0px;">
                                                <img style="display:block; line-height:0px; font-size:0px; border:0px;"
                                                    src="https://www.pngkey.com/png/full/855-8558025_university-of-greenwich-logo.png"
                                                    width="auto" height="100" alt="logo">
                                            </td>
                                        </tr>
                                    </tbody>
                                </table>
                            </td>
                        </tr>
                    </tbody>
                </table>
            </td>
        </tr>

        <tr>
            <td align="center">
                <table class="col-600" width="600" border="0" align="center" cellpadding="0" cellspacing="0"
                    style="margin-left:20px; margin-right:20px;">


                    <tbody>
                        <tr>
                            <td align="center">
                                <table class="col-600" width="600" border="0" align="center" cellpadding="0"
                                    cellspacing="0"
                                    style=" border-left: 1px solid #dbd9d9; border-right: 1px solid #dbd9d9; border-bottom: 1px solid #dbd9d9;">
                                    <tbody>
                                        <tr>
                                            <td height="50"></td>
                                        </tr>
                                        <tr>
                                            <td>

                                                <table width="100%" border="0" align="center" cellpadding="0"
                                                    cellspacing="0" class="col2" style="">
                                                    <tbody>
                                                        <tr>
                                                            <td align="center">
                                                                <table class="insider" width="90%" border="0"
                                                                    align="center" cellpadding="0" cellspacing="0">


                                                                    <tbody>
                                                                        <tr align="left">
                                                                            <td
                                                                                style="font-family: 'Raleway', sans-serif; font-size:23px; color:#2a3b4c; line-height:30px; font-weight: bold;">
                                                                                Hi!
                                                                            </td>
                                                                        </tr>

                                                                        <tr>
                                                                            <td height="5"></td>
                                                                        </tr>
                                                                        <tr align="left">
                                                                            <td
                                                                                style="font-family: 'Raleway', sans-serif; font-size:23px; color:#2a3b4c; line-height:30px; font-weight: bold;">
                                                                                {{ fromEmail }} - {{ fromPhoneNumber }}
                                                                            </td>
                                                                        </tr>
                                                                        <tr>
                                                                            <td
                                                                                style="font-family: 'Lato', sans-serif; font-size:16px; color:#7f8c8d; line-height:24px; font-weight: 300;">
                                                                                {{ message }}
                                                                                <br>
                                                                                Thank you!
                                                                            </td>
                                                                        </tr>
                                                                    </tbody>
                                                                </table>
                                                            </td>
                                                        </tr>
                                                    </tbody>
                                                </table>
                                            </td>
                                        </tr>
                                        <tr>
                                            <td height="50"></td>
                                        </tr>
                                    </tbody>
                                </table>
                            </td>
                        </tr>
                    </tbody>
                </table>
            </td>
        </tr>
    </tbody>
</table>